apiVersion: batch/v1
kind: Job
metadata:
  name: {{{ job_name }}}
  labels:
    dummysite: "{{ dummysite_name }}"
    websiteurl: "{{{ websiteurl }}}"
spec:
  template:
    spec:
      containers:
      - name: {{{ container_name }}}
        image: {{{ image }}}
        args: ["{{{ websiteurl }}}"]
      restartPolicy: Never
